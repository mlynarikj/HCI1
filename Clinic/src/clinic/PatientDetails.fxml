<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="386.0" prefWidth="588.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="clinic.PatientDetailsController">
  <bottom>
    <HBox alignment="CENTER" prefHeight="140.0" prefWidth="588.0">
      <children>
        <ImageView fx:id="image" fitHeight="125.25" fitWidth="167.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="cancel" mnemonicParsing="false" onMouseClicked="#cancel" text="Cancel">
          <HBox.margin>
            <Insets left="50.0" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
  </bottom>
  <center>
    <ListView fx:id="list" prefHeight="186.0" prefWidth="369.0" />
  </center>
  <left>
    <VBox alignment="CENTER_LEFT" prefHeight="186.0" prefWidth="144.0">
      <children>
        <Label text="Name">
          <VBox.margin>
            <Insets bottom="10.0" />
          </VBox.margin>
        </Label>
        <Label fx:id="name" text="Label">
          <VBox.margin>
            <Insets bottom="30.0" />
          </VBox.margin>
        </Label>
        <Label text="Surname" />
        <Label fx:id="surname" text="Label" />
      </children>
    </VBox>
  </left>
  <top>
    <HBox alignment="CENTER" prefHeight="64.0" prefWidth="588.0">
      <children>
        <Label text="Patient Details" />
      </children>
    </HBox>
  </top>
</BorderPane>
