<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="clinic.MainWindowController">
  <children>
    <BorderPane layoutX="0.0" layoutY="0.0" prefHeight="400.0" prefWidth="600.0">
      <center>
        <Label text="Welcome to Primary Care Clinic">
          <font>
            <Font name="System Bold" size="20.0" />
          </font>
        </Label>
      </center>
      <top>
        <ToolBar prefHeight="75.0" prefWidth="600.0">
          <items>
            <Button fx:id="patients" mnemonicParsing="false" onMouseClicked="#patients" text="Patients" />
            <Button id="doctor" fx:id="doctors" mnemonicParsing="false" onMouseClicked="#doctors" text="Doctors" />
            <Button fx:id="appointments" mnemonicParsing="false" onMouseClicked="#appointments" text="Appointments" />
          </items>
        </ToolBar>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
